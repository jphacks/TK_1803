

<div class="jumbotron" style="padding:40px; background-image: url('http://craigwolf.com/news/uploaded_images/Band-2007-760222.jpg');" >

  <div class="relative2">
    <div class="container" style="border-radius: 13px; border: 1px solid #ffffff; width:94px">
      <font color="white"> Sign Out </font>
    </div>
  </div>

  <div class="relative3">
    <div class="row">
      <a href="" class="icon fa-twitter"></a> |
      <a href="" class="icon fa-instagram"></a> | 
      <a href="" class="icon fa-facebook"></a>
    </div>
  </div>

  <div class="relative1">
    <h1> <font color="white"> 
      <%= @user.name %> 
    </font></h1>   
  </div>

  <div class="relative">
    <img src="data:image/jpeg;base64," alt="Avatar" class="avatar">
  </div>

  <div class="container" style="height:200px"> </div>
</div>
  

<% i = 0 %>
<% @video.each do |video| %>
  <% if i%3==0 %>
    <% '<div class="row">' %>
  <% end %>

  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <iframe width="100%" height="265" src="<%=video.url.sub(/watch\?v=/, "embed/")%>" frameborder="0" allowfullscreen></iframe>

      <div class="caption">

        <div class="row">
          <div class="col-md-1"></div>
          <div class="col-md-8">
            <p>
              <span style="font-size: 28px;"><%= video.name %></span><br>
              <i class="fa fa-music"></i>
              <span><%= video.score %></span><br>
              <span class="oi oi-tag"></span>
              <span><%= video.instrument%></span><br>
              <span class="oi oi-clock"></span>
              <span><%= video.date %></span><br>
            </p>
          </div>

          <div class="col-md-1">
            <ul class="icons">
              <li><a href="#" class="icon fa-twitter"><span class="label">Twitter</span></a>
              <li><a href="#" class="icon fa-facebook"><span class="label">Facebook</span></a>
              <li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a>
            </ul>
          </div>
        </div >

      </div>
    </div>
  </div>

  <% if i%3==0 %>
    <% '</div>' %>
  <% end %>
  <% i+=1 %>
<% end %>


<% if current_user.id == @user.id %>

  <% if i%3==0 %>
    <% '<div class="row">' %>
  <% end %>

    <div class="col-sm-6 col-md-4">
      <div class="thumbnail" >
        <div style= "height : 445px; text-align:center; vertical-align: middle; line-height: 445px;">
          <a href = "http://localhost:3000/videos/new">
            <i class="fa fa-plus" style="font-size: 96px; "></i>
          </a>
      </div>
    </div>

  <% if i%3==0 %>
    <% '</div>' %>
  <% end %>

 <% end %>